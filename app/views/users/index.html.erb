<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>
<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= link_to user.name, user %> signed up <%= user.created_at.to_date %>
      <%= link_to 'Edit', edit_user_path(user) %>
    </li>
  <% end %>
</ul>

<%= will_paginate %>
